<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
    <plugin>
        <pluginName>Classparser-Plugin</pluginName>
        <pluginDescription>Parsing all the classes. Just install this Plugin and a Classhelper object will appear. There you can add classes and their description. Use this plugin to enable the Admin to change classes in the backend.</pluginDescription>
        <pluginNiceName>Classparser</pluginNiceName>
        <pluginIcon>/plugins/Classparser/static/img/icon.png</pluginIcon>
        <pluginVersion>1.1.0</pluginVersion>
        <pluginRevision>1</pluginRevision>
        <pluginBuildTimestamp></pluginBuildTimestamp>
        <pluginClassName>Classparser_Plugin</pluginClassName>
        <pluginIncludePaths>
            <path>/Classparser/lib</path>
            <path>/Classparser/models</path>
        </pluginIncludePaths>
        <pluginNamespaces>
            <namespace>Classparser</namespace>
        </pluginNamespaces>
        <pluginDocumentEditmodeJsPaths>
            <path>/Classparser/static/js/pimcore/document/tags/classparser.js</path>
        </pluginDocumentEditmodeJsPaths>
    </plugin>
</zend-config>
